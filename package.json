{
    "name": "my-portfolio-root",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "export": "next export",
        "install:frontend": "npm --prefix __version2__/frontend install",
        "install:backend": "npm --prefix __version2__/backend install",
        "install:all": "npm run install:frontend && npm run install:backend",
        "dev:frontend": "npm --prefix __version2__/frontend run dev",
        "dev:backend": "npm --prefix __version2__/backend run dev || echo 'no backend dev script'",
        "dev:all": "npm-run-all --parallel dev dev:frontend dev:backend"
    },
    "dependencies": {
        "next": "14.0.4",
        "react": "^18",
        "react-dom": "^18"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "autoprefixer": "^10.0.1",
        "eslint": "^8",
        "eslint-config-next": "14.0.4",
        "postcss": "^8",
        "tailwindcss": "^3.3.0",
        "typescript": "^5",
        "npm-run-all": "^4.1.5"
    }
}